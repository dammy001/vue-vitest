{
  "name": "vue-vitest",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint:fix": "eslint --ext .js,.vue,.ts . --fix",
    "format": "prettier --write \"./src/**/*.{ts,vue,js}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,vue,js}\"",
    "ci": "pnpm typecheck && pnpm lint && pnpm build && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "2.0.13",
    "vue": "3.2.33"
  },
  "devDependencies": {
    "@damilaredev/eslint-config-vue": "0.0.13",
    "@vitejs/plugin-vue": "2.3.1",
    "@vue/test-utils": "2.0.0-rc.20",
    "autoprefixer": "10.4.4",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "happy-dom": "2.55.0",
    "husky": "7.0.4",
    "postcss": "8.4.12",
    "prettier": "2.6.2",
    "sass": "1.50.1",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3",
    "vite": "2.9.5",
    "vitest": "0.9.4",
    "vue-tsc": "0.34.7"
  },
  "engines": {
    "node": ">=14"
  }
}