{
  "name": "vue-vitest",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint:fix": "eslint --ext .js,.vue,.ts . --fix",
    "format": "prettier --write \"./src/**/*.{ts,vue,js}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,vue,js}\"",
    "ci": "pnpm typecheck && pnpm lint && pnpm build && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "2.0.27",
    "vue": "3.2.41"
  },
  "devDependencies": {
    "@damilaredev/eslint-config-vue": "0.0.19",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.13",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.6.0",
    "happy-dom": "8.9.0",
    "husky": "8.0.3",
    "postcss": "8.4.21",
    "prettier": "2.8.8",
    "sass": "1.57.1",
    "tailwindcss": "3.4.17",
    "typescript": "4.9.5",
    "vite": "2.9.18",
    "vitest": "0.28.3",
    "vue-tsc": "0.40.13"
  },
  "engines": {
    "node": ">=14"
  }
}