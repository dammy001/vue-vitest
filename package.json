{
  "name": "vue-vitest",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "lint": "eslint --ext .js,.vue,.ts .",
    "lint:fix": "eslint --ext .js,.vue,.ts . --fix",
    "format": "prettier --write \"./src/**/*.{ts,vue,js}\"",
    "format:check": "prettier --check \"./src/**/*.{ts,vue,js}\"",
    "ci": "pnpm typecheck && pnpm lint && pnpm build && pnpm test",
    "prepare": "husky install"
  },
  "dependencies": {
    "pinia": "2.0.17",
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@damilaredev/eslint-config-vue": "0.0.16",
    "@vitejs/plugin-vue": "2.3.4",
    "@vue/test-utils": "2.0.2",
    "autoprefixer": "10.4.8",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "happy-dom": "3.1.1",
    "husky": "8.0.1",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "sass": "1.54.3",
    "tailwindcss": "3.0.24",
    "typescript": "4.7.4",
    "vite": "2.9.9",
    "vitest": "0.10.5",
    "vue-tsc": "0.34.11"
  },
  "engines": {
    "node": ">=14"
  }
}